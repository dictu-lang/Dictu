/**
 * inherit.du
 *
 * Testing class inheritance
 */

class Base {
    var classVar = 10;

    init() {
        this.x = 100;
    }

    func() {
        return 10;
    }
}

class Test < Base {}

class Testing < Base {
    init() {
        this.value = super.func();
        this.newFunc = super.func;
    }
}

assert(Test().func() == 10);
assert(Testing().value == 10);
assert(Testing().classVar == 10);
assert(Testing().newFunc() == 10);