/**
 * fall.du
 *
 * Testing fall within a switch
 */
from UnitTest import UnitTest;

class TestMultiCase < UnitTest {
    testFallWithCase() {
        var x = 9;

        switch (x) {
            case 1, 2, 3, 4: {
                x += 10;
            }

            case 5, 6, 7, 8, 9: {
                x += 10;
            }

            fall case 10: {
                x += 10;
            }
        }

        this.assertEquals(x, 29);
    }

    testFallWithDefault() {
        var x = 10;

        switch (x) {
            case 1, 2, 3, 4: {
                x += 10;
            }

            case 5, 6, 7, 8, 9: {
                x += 10;
            }

            case 10: {
                x += 10;
            }

            fall default: {
                x += 100;
            }
        }

        this.assertEquals(x, 120);
    }
}

TestMultiCase().run();